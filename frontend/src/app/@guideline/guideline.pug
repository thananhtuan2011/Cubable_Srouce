//- cub-flowchart(
//- 	#flowchart,
//- 	[rootNode]='rootNode',
//- 	(scrolling)='console.log( $event )',
//- 	(scaling)='console.log( $event )',
//- 	(backdropClicked)='console.log( "Backdrop clicked", $event )',
cub-flowchart(
	#flowchart,
	[rootNode]='rootNode',
	[viewOnly]='false',
	(nodeAddon)='addNode( $event )',
	(nodeClicked)='console.log( "Node clicked", $event )')
	ng-template(
		cubFlowchartNodeTemplate,
		type='sub-process',
		let-cmp='cmp',
		let-node='node',
		let-isCollapsed='isCollapsed')
		.layout-row.layout-align-space-between.layout-gap-6
			button(cubButton='secondary')
				| {{ node.metadata.label }}
			button(
				cubButton='secondary',
				icon='expand',
				iconSize='20px',
				[icon]='isCollapsed ? "expand" : "collapse"',
				(click)='isCollapsed ? cmp.expand() : cmp.collapse()')
	ng-template(
		cubFlowchartNodeTemplate,
		type='trigger',
		let-cmp='cmp',
		let-node='node')
		cub-card.w-280.mih-84(
			clickable,
			size='small',
			boxShadow='on')
			cub-card-header(
				style='color: white; background: linear-gradient(90deg, #54A158 0.02%, #8DDD8B 96.38%);')
				.layout-row.layout-align-space-between-center.flex
					| {{ node.type }}
					button(
						cubBasicButton,
						icon='trash',
						iconSize='20px',
						*ngIf='!flowchart.viewOnly && node !== rootNode && !node.disableInsertNextNode',
						(click)='flowchart.destroyNode( node )')
			cub-card-content
				.text-center
					| {{ node.metadata.label }}
	ng-template(
		cubFlowchartNodeTemplate,
		type='condition',
		let-node='node')
		cub-card.w-280.mih-84(
			clickable,
			size='small',
			boxShadow='on')
			cub-card-header(
				style='color: white; background: linear-gradient(90deg, #ED4C7A 11.58%, #FF8EA9 96.38%);')
				.layout-row.layout-align-space-between-center.flex
					| {{ node.type }}
					button(
						cubBasicButton,
						icon='trash',
						iconSize='20px',
						*ngIf='!flowchart.viewOnly && node !== rootNode && !node.disableInsertNextNode',
						(click)='flowchart.destroyNode( node )')
			cub-card-content
				.text-center
					| {{ node.metadata.label }}
	ng-template(
		cubFlowchartNodeTemplate,
		type='action',
		let-node='node')
		cub-card.w-280.mih-84(
			clickable,
			size='small',
			boxShadow='on')
			cub-card-header(
				style='color: white; background: linear-gradient(90deg, #545ADA 30.34%, #56B6FF 118.92%);')
				.layout-row.layout-align-space-between-center.flex
					| {{ node.type }}
					button(
						cubBasicButton,
						icon='trash',
						iconSize='20px',
						*ngIf='!flowchart.viewOnly && node !== rootNode && !node.disableInsertNextNode',
						(click)='flowchart.destroyNode( node )')
			cub-card-content
				.text-center
					| {{ node.metadata.label }}

